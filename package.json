    "scss",
    "sass",
    "js",
    "json",
    "vars",
    "variables",
    "webpack",
    "loader"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epegzz/sass-vars-loader"
  },
  "engineStrict": true,
  "main": "dist/sassVarsLoader.js",
  "scripts": {
